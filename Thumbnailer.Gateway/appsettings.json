{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Clusters": {
      "thumbnail-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5147/"
          }
        }
      }
    },
    "Routes": {
      "route1": {
        "ClusterId": "thumbnail-cluster",
        "Match": {
          "Path": "api/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      }
    }
  }
}
